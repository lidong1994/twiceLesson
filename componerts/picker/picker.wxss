.con {
  width: 100%;
}

.picker_cons {
  border-radius: 10rpx;
  background: #fff;
  margin: auto;
}

.picker_list {
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: #999;
  font-size: 32rpx;
  border-bottom: 1px solid #e2e2e2;
}

.picker_title {
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 35rpx solid #f5f5f5;
  margin-top: 50rpx;
}

.picker_title text {
  font-size: 38rpx;
  color: #333;
}

.ys-dialog {
  position: fixed;
  z-index: 5000;
  width: 95%;
  bottom: -25%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  text-align: center;
  border-radius: 10rpx;
}
.ys-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.4);
}

.picker_title_bottom {
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  margin-top: 30rpx;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
}

.active {
  color: #ff8432;
}
