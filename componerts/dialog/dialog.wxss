.ys-dialog {
  position: fixed;
  z-index: 5000;
  width: 95%;
  max-width: 300px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  border-radius: 10rpx;
  background: #fff;

}
.con{
  width: 100%;
  background: #fff;
}
.ys-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.4);
}
.dialog_content{
  width: 100%;
  text-align: center;
  padding-top: 20rpx;
}
.dialog_content text{
  font-size: 35rpx;
}

.youhuiquan{
  font-size: 28rpx;
color: #333333;
padding:30rpx;
}
.youhuiquan view{
  padding: 10rpx;
}
.x{
  position: fixed;
  bottom: -19%;
  width: 64rpx;
  height: 64rpx;
  left: 43%;
}